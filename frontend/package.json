{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --port 8080",
    "prod": "npm run build && npm run prod:serve",
    "build": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "build:prerender": "vite-plugin-ssr prerender",
    "build:clean": "rimraf dist/",
    "build:vite": "vite build && vite build --ssr",
    "prod:serve": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@unocss/preset-uno": "^0.39.0",
    "@unocss/transformer-directives": "^0.39.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "@vueuse/core": "^8.6.0",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "d3": "^7.4.4",
    "d3-delaunay": "^6.0.2",
    "delaunator": "^5.0.0",
    "express": "^4.18.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-prism": "^2.2.4",
    "markdown-it-theme": "^0.4.0",
    "pinia": "^2.0.14",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "serve": "^13.0.2",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.3",
    "unocss": "^0.39.0",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-md": "^0.14.1",
    "vite-plugin-pages": "^0.24.1",
    "vite-plugin-ssr": "^0.3.64",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue": "^3.2.25",
    "vue-axios": "^3.4.1",
    "vue-router": "^4.0.16"
  }
}
